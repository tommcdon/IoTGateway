    <file src="bin\x86\Release\Waher.Things.Arduino.dll" target="runtimes\win10-x86\lib\uap10.0" />
    <file src="bin\x86\Release\Waher.Things.Arduino.pdb" target="runtimes\win10-x86\lib\uap10.0" />

    <file src="bin\x64\Release\Waher.Things.Arduino.dll" target="runtimes\win10-x64\lib\uap10.0" />
    <file src="bin\x64\Release\Waher.Things.Arduino.pdb" target="runtimes\win10-x64\lib\uap10.0" />

    <file src="bin\arm\Release\Waher.Things.Arduino.dll" target="runtimes\win10-arm\lib\uap10.0" />
    <file src="bin\arm\Release\Waher.Things.Arduino.pdb" target="runtimes\win10-arm\lib\uap10.0" />
  </files>
