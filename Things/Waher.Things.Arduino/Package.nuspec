    <dependencies>
      <dependency id="Waher.Content" version="1.0.24"/>
      <dependency id="Waher.Events" version="1.0.11"/>
      <dependency id="Waher.IoTGateway" version="1.0.89"/>
      <dependency id="Waher.Networking.XMPP" version="1.0.45"/>
      <dependency id="Waher.Persistence" version="1.0.15"/>
      <dependency id="Waher.Runtime.Inventory" version="1.0.12"/>
      <dependency id="Waher.Runtime.Language" version="1.0.2"/>
      <dependency id="Waher.Script" version="1.0.35"/>
      <dependency id="Waher.Things" version="1.0.28"/>
      <dependency id="Waher.Things.Metering" version="1.0.20"/>
      <dependency id="Windows-Remote-Arduino" version="1.4.0"/>
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\Images\Icon_64x64.png" target="Meta" />
    <file src="..\..\LICENSE.txt" target="Meta" />

